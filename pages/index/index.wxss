.popUpWindow{
  width: 500rpx;
  height: 300rpx;
  border-radius: 15rpx;
  position: fixed;
  top:50%;
  left:50%;
  transform:translateX(-50%) translateY(-50%);
  -ms-transform:translateX(-50%) translateY(-50%);
  -moz-transform:translateX(-50%) translateY(-50%);
  -webkit-transform:translateX(-50%) translateY(-50%);
  -o-transform:translateX(-50%) translateY(-50%);
  display: flex;
  flex-direction: column;
  z-index: 999;
  background-color: white;
  opacity: 1 !important;
}
.t_popUpWindow{
  height: 80rpx;
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
  justify-content: center;
}
.m_popUpWindow{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-left: 20rpx;
  padding-right: 20rpx;
  font-size: 32rpx;
}
.b_popUpWindow{
  height: 80rpx;
  border-top: 1px solid #eee;
  display: flex;
  align-items: center;
  justify-content: center;
}
.mask{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: black;
  opacity: 0.7;
  z-index: 10;
}
