/* pages/center/center.wxss */
.header{
  width: 100%;
  height: 435rpx;
  background-image: url("data:image/png;base64,");
  background-size: 750rpx 435rpx;
  background-repeat: no-repeat;
  background-position: center;
}
.peopleMsg{
  width: 100%;
  height: 260rpx;
  display: flex;
  /* border-bottom: 1px solid black; */
}
.l_peopleMsg{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.m_peopleMsg{
  flex: 2;
  display: flex;
  align-items: center;
  padding-left: 10rpx;
}
.r_peopleMsg{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.headIcon{
  width: 154rpx;
  height: 154rpx;
  border-radius: 100%;
}
.userName{
  display: block;
  font-size: 40rpx;
  font-weight: bold;
}
.userAcount{
  display: block;
  font-size: 30rpx;
}
.peopleData{
  width: 100%;
  height: 125rpx;
  display: flex;
}
.l_peopleData,
.m_peopleData,
.r_peopleData{
  flex: 1;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  font-weight: bold;
}
.alignCenter{
  text-align: center;
}
.entry{
  padding-top: 120rpx;
}
.row{
  display: flex;
  margin-bottom: 30rpx;
}
.l_row,
.r_row{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.entryImg{
  width: 307rpx;
  height: 136rpx;
}
.editIcon{
  width: 45rpx;
  height: 45rpx;
}